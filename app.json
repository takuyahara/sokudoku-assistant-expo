{
  "expo": {
    "name": "速読",
    "slug": "SokudokuAssistant",
    "description": "一定時間ごとにバイブレーションでページめくりを促す読書強制、および速読修得を目的としたアプリ。",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.takuyahara.sokudokuassistant"
    }
  }
}
